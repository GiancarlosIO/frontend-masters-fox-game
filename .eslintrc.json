{
  "extends": [
    "eslint-config-webpack-nexus"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-expressions": "off"
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
